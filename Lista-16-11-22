/exercicio 1
select   sexo  , nome 

from alunos

where sexo='M'



/exercicio 2
select nome 

from professores

where codprof='1'



/exercicio 3
select CARGAHORARIA, nome

from disciplinas

where CARGAHORARIA>=60



/exercicio 4
select qtd_creditos, nome

from disciplinas

where QTD_CREDITOS not between 4 and 6


/exercicio 5
select  codprof, nome, coddepto
from professores
where nome= 'ZE'

/exercicio 6
select nome
from professores
order by nome

/exercicio 7
