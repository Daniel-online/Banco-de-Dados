/exercicio 8
select nome, sexo, dat_nasc
from alunos
where dat_nasc between '01/01/1990' and '01/01/2000'

/ exercicio 9
select matricula
from diarios
where av3>9 and av1<5
/exercicio 10
select matricula, codturma
from diarios
order by codturma asc, matricula asc
